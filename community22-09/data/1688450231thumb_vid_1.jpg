function getThumb(){
    
    const video = document.getElementById('my_video');
    const thecanvas = document.getElementById('thecanvas');
    const img = document.getElementById('thumbnail_img');
    
    video.addEventListener('loadedmetadata', function(){
      setTimeout(() => draw(video, thecanvas, img), 500)
    }, false);
    
    video.currentTime = 2;
    video.load();
    
    function draw( video, thecanvas, img ){
    	thecanvas
      	.getContext('2d')
        .drawImage( video, 0, 0, thecanvas.width, thecanvas.height);
        
      img.setAttribute('src', thecanvas.toDataURL());
    }
    
    
    const img2 = document.getElementById('thumbnail_img')

    fetch(img2.src)
    .then(res => res.blob())
    .then(blob => {
      const file = new File([blob], 'thumb.png', blob)
      
      const dataTransfer = new DataTransfer();
            dataTransfer.items.add(file);
             document.getElementById('input_file_canvas_thumb').files = dataTransfer.files;
    });
    


 }

    
